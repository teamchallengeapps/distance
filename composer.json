{
    "name": "teamchallengeapps/distance",
    "authors": [
        {
            "name": "David Rushton",
            "email": "david@bigteamchallenge.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-intl": "*",
        "brick/math": "^0.12",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^8.26"
    },
    "autoload": {
        "psr-4": {
            "TeamChallengeApps\\Distance\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TeamChallengeApps\\Distance\\DistanceServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "autoload-dev": {
        "psr-4": {
            "TeamChallengeApps\\Distance\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    }
}
